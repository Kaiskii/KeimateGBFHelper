{
    "name": "Keimate",
    "version": "1.0",
    "description": "Fuck KMR",
    "manifest_version": 2,
	"permissions": ["activeTab", "declarativeContent", "storage","tabs"],
	"background": {
      "scripts": ["background.js", "js/keimate/keimate-minified.js"],
      "persistent": false
    },
	"options_page": "options.html",
	"page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
	"icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
	"web_accessible_resources": [ 
		"css/keimate/keimate-minified.css",
		"js/keimate/keimate-minified.js"
	],
	"content_scripts": [
		{
		  "matches": ["http://game.granbluefantasy.jp/*"], 
		  "js": ["js/keimate/keimate-minified.js", "js/contentScripts.js"],
		  "css" : ["css/keimate/keimate-minified.css"]
		}
	]
}